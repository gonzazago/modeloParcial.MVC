<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> @ViewBag.Title</title>
    <link href="~/Content/materialize.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/materialize.js" type="text/javascript"></script>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    </head>
<body>
    <nav>
        <div class="nav-wrapper indigo darken-4">
            <a href="#" class="brand-logo">Modelo Parcial !</a>
            <ul id="nav-mobile" class="right hide-on-med-and-down">
                <li><a href="/Home/ListarEjercicios">Listado de Ejercicios <i class="view_list"></i></a></li>
                <li><a href="/Home/Nuevo">Nuevo Ejercicio</a></li>
            </ul>
        </div>
    </nav>
    <div class="container">
        @RenderBody()
    </div>
    <footer class="page-footer indigo darken-4">
        <div class="footer-copyright">
            <p class="center-align">&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </div>
    </footer>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script>

        $(document).ready(function () {
            $('select').material_select();
        });
    </script>
</body>
</html>



